#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;

    vector<int> arr(n);
    for (int i = 0; i < n; i++) cin >> arr[i];

    int X;
    cin >> X;

    int low = 0, high = n - 1, ans = -1;

    while (low <= high) {
        int mid = low + (high - low) / 2;

        if (arr[mid] == X) {
            ans = mid;
            break;
        }
        else if (arr[mid] < X)
            low = mid + 1;
        else
            high = mid - 1;
    }

    cout << ans;
    return 0;
}
