#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;

    vector<vector<int>> graph(n, vector<int>(n));
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            cin >> graph[i][j];

    int src;
    cin >> src;

    vector<int> dist(n, INT_MAX);
    vector<int> visited(n, 0);
    dist[src] = 0;

    for (int c = 0; c < n - 1; c++) {
        int u = -1, mn = INT_MAX;

        for (int i = 0; i < n; i++)
            if (!visited[i] && dist[i] < mn)
                mn = dist[i], u = i;

        visited[u] = 1;

        for (int v = 0; v < n; v++) {
            if (graph[u][v] != 0 && !visited[v] &&
                dist[u] != INT_MAX &&
                dist[u] + graph[u][v] < dist[v])
            {
                dist[v] = dist[u] + graph[u][v];
            }
        }
    }

    for (int x : dist)
        cout << (x == INT_MAX ? -1 : x) << " ";
}
